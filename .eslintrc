{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "arrowParens": "avoid",
        "printWidth": 80,
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
}
